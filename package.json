{
  "name": "timiks",
  "version": "1.0.0",
  "description": "Rubik's cube timer",
  "main": "index.js",
  "scripts": {
    "copy-static": "mkdir -p public; cp src/copy-static/* public/",
    "watch": "npm run copy-static; webpack-dev-server",
    "test": "eslint src",
    "build": "npm run copy-static; NODE_ENV=production webpack",
    "precommit": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngerritsen/timiks.git"
  },
  "keywords": [
    "rubiks",
    "cube",
    "timer"
  ],
  "author": "Niels Gerritsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngerritsen/timiks/issues"
  },
  "homepage": "https://github.com/ngerritsen/timiks#readme",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-pro-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "element-closest": "^2.0.2",
    "nosleep.js": "^0.7.0",
    "polished": "^1.9.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "shortid": "^2.2.8",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.0",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "react"
    ]
  }
}
